<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/skyblue"
    tools:context="com.example.hamshi.iems.SymptomlistActivity">
    <TextView

        android:layout_width="93dp"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:text="name"
        android:textSize="25dp"
        android:background="#2a98ff"
        android:id="@+id/textView13" />
    <TextView


        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:text="subbody"
        android:textSize="25dp"
        android:layout_weight="0.18"
        android:id="@+id/textView14"
        android:background="#2a98ff"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView13"
        android:layout_toEndOf="@+id/textView13" />

    <TextView

        android:layout_width="78dp"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:text="symptom"
        android:textSize="25dp"
        android:layout_weight="0.41"
        android:background="#2a98ff"
        android:id="@+id/textView11"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView14"
        android:layout_toLeftOf="@+id/textView15"
        android:layout_toStartOf="@+id/textView15" />

    <TextView

        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:text="disease"
        android:textSize="25dp"
        android:background="#2a98ff"
        android:layout_weight="0.34"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/textView12"
        android:layout_toStartOf="@+id/textView12"
        android:id="@+id/textView15" />

    <TextView

        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:textColor="@android:color/black"
        android:text="dname"
        android:textSize="25dp"
        android:background="#2a98ff"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/linearLayout"
        android:layout_alignEnd="@+id/linearLayout"
        android:id="@+id/textView12" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/linearLayout"
        android:layout_below="@+id/textView13">


    </LinearLayout>

    <ListView
        android:id="@+id/Doctor_listView"
        android:layout_height="436dp"
        android:layout_width="fill_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/linearLayout" />

</RelativeLayout>
